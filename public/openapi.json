{
    "openapi": "3.0.0",
    "info": {
        "title": "API Reservas medicas",
        "description": "API para gestionar las reservas medicas",
        "version": "1.0.0"
    },
    "paths": {
        "/reservas": {
            "get": {
                "tags": [
                    "Reservas"
                ],
                "summary": "Obtener todas las reservas",
                "operationId": "cd8e440563d14964315c801208138882",
                "responses": {
                    "200": {
                        "description": "Lista todas las reservas existentes",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/RespuestaReserva"
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "No hay reservas",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/reservas/mis-reservas": {
            "get": {
                "tags": [
                    "Reservas"
                ],
                "summary": "Obtener las reservas de un usuario ya sea profesional o paciente",
                "operationId": "f1ce477f477051b0c069e5230ab7f2f1",
                "responses": {
                    "200": {
                        "description": "Lista de las reservas de un usuario dado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/RespuestaReserva"
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "No hay reservas",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/reservas/reservar": {
            "post": {
                "tags": [
                    "Reservas"
                ],
                "summary": "Realizar una reserva",
                "operationId": "c05cc654cc7ff84e840917f42ed8f501",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Reserva"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Reserva realizada",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": [
                                    "EXITO => Su reserva ha sido procesada correctamente!"
                                ]
                            }
                        }
                    },
                    "404": {
                        "description": "No hay resultado",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": {
                                    "ERROR": "No se encontro un profesional con ese id"
                                }
                            }
                        }
                    },
                    "409": {
                        "description": "Reserva superpuesta",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": {
                                    "ERROR": "Este usuario ya tiene una reserva para esa misma fecha"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/reservas/cancelar/{id}": {
            "put": {
                "tags": [
                    "Reservas"
                ],
                "summary": "Cancelar una reserva",
                "operationId": "a6819a8d222efde6ad1382678d8e6575",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Reserva cancelada!",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": {
                                    "EXITO": "Reserva cancelada!"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Cancelacion tardia",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": {
                                    "ERROR": "Solo se puede cancelar una reserva con Xhs de anticipacion"
                                }
                            }
                        }
                    },
                    "409": {
                        "description": "La reserva ya fue cancelada o completada",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": {
                                    "ERROR": "La reserva ya fue cancelada!"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Error del servidor",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": {
                                    "ERROR": "Error Interno del Servidor!"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "Paciente": {
                "required": [
                    "nombre",
                    "apellido",
                    "email",
                    "telefono"
                ],
                "properties": {
                    "nombre": {
                        "type": "string",
                        "example": "Juan"
                    },
                    "apellido": {
                        "type": "string",
                        "example": "Perez"
                    },
                    "email": {
                        "type": "string",
                        "example": "juanPerez@gmail.com",
                        "nullable": true
                    },
                    "telefono": {
                        "type": "string",
                        "example": "0118574892",
                        "nullable": true
                    }
                },
                "type": "object"
            },
            "Profesional": {
                "required": [
                    "nombre",
                    "apellido",
                    "profesion",
                    "email",
                    "telefono"
                ],
                "properties": {
                    "nombre": {
                        "type": "string",
                        "example": "Roberto"
                    },
                    "apellido": {
                        "type": "string",
                        "example": "Falcao"
                    },
                    "profesion": {
                        "type": "string",
                        "example": "Odontologo"
                    },
                    "email": {
                        "type": "string",
                        "example": "roberFalcao@outlook.com",
                        "nullable": true
                    },
                    "telefono": {
                        "type": "string",
                        "example": "0984728910",
                        "nullable": true
                    },
                    "password": {
                        "type": "string",
                        "example": "Rober13#"
                    }
                },
                "type": "object"
            },
            "Reserva": {
                "required": [
                    "idProfesional",
                    "fecha"
                ],
                "properties": {
                    "idProfesional": {
                        "type": "integer",
                        "example": 1
                    },
                    "fecha": {
                        "type": "string",
                        "example": "2026-03-15 18:45:00"
                    },
                    "estado": {
                        "type": "string",
                        "example": "Confirmada"
                    }
                },
                "type": "object"
            },
            "RespuestaPaciente": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 10
                    },
                    "nombre": {
                        "type": "string",
                        "example": "Alonso Martinez"
                    },
                    "email": {
                        "type": "string",
                        "example": "aMartinez@gmail.com",
                        "nullable": true
                    },
                    "telefono": {
                        "type": "string",
                        "example": "1234567890",
                        "nullable": true
                    }
                },
                "type": "object"
            },
            "RespuestaProfesional": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 12
                    },
                    "nombre": {
                        "type": "string",
                        "example": "Lucero Gimenez"
                    },
                    "profesion": {
                        "type": "string",
                        "example": "Pediatra"
                    },
                    "email": {
                        "type": "string",
                        "example": "luceroG@outlook.com",
                        "nullable": true
                    },
                    "telefono": {
                        "type": "string",
                        "example": "347586901",
                        "nullable": true
                    }
                },
                "type": "object"
            },
            "RespuestaReserva": {
                "properties": {
                    "idReserva": {
                        "type": "integer",
                        "example": 15
                    },
                    "idPaciente": {
                        "type": "integer",
                        "example": "24"
                    },
                    "idProfesional": {
                        "type": "integer",
                        "example": "12"
                    },
                    "fecha": {
                        "type": "string",
                        "example": "2026-04-18 15:00:00"
                    },
                    "estado": {
                        "type": "string",
                        "example": "Confirmada"
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "description": "Ingresa tu token JWT aqu√≠ para autenticarte",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "tags": [
        {
            "name": "Reservas",
            "description": "Reservas"
        }
    ]
}